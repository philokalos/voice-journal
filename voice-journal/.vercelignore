# Vercel deployment ignore file

# Development files
.env.local
.env.development
.env.test

# Node modules
node_modules/

# Build outputs (will be built by Vercel)
dist/
build/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Editor files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Temporary files
.tmp/
.cache/

# Documentation (not needed in production)
docs/
*.md
!README.md

# Test files
tests/
test/
**/*.test.*
**/*.spec.*

